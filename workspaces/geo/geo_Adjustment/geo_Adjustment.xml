<?xml version="1.0" encoding="UTF-8"?>
<as:AppSchemaDataAccess xmlns:as="http://www.geotools.org/app-schema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.geotools.org/app-schema AppSchemaDataAccess.xsd">
    <namespaces>
        <Namespace>
            <prefix>gml</prefix>
            <uri>http://www.opengis.net/gml/3.2</uri>
        </Namespace>
        <Namespace>
            <prefix>gmd</prefix>
            <uri>http://www.isotc211.org/2005/gmd</uri>
        </Namespace>
        <Namespace>
            <prefix>geo</prefix>
            <uri>urn:xml-gov-au:icsm:egeodesy:0.2</uri>
        </Namespace>
    </namespaces>
    <sourceDataStores>
        <DataStore>
            <id>surveymarks</id>
            <parameters>
                <Parameter>
                    <name>dbtype</name>
                    <value>postgisng</value>
                </Parameter>
                <Parameter>
                    <name>host</name>
                    <value>localhost</value>
                </Parameter>
                <Parameter>
                    <name>port</name>
                    <value>5432</value>
                </Parameter>
                <Parameter>
                    <name>database</name>
                    <value>surveymarks</value>
                </Parameter>
                <Parameter>
                    <name>user</name>
                    <value>postgres</value>
                </Parameter>
                <Parameter>
                    <name>passwd</name>
                    <value>postgres</value>
                </Parameter>
                <Parameter>
                    <name>Expose primary keys</name>
                    <value>true</value>
                </Parameter>
            </parameters>
        </DataStore>
    </sourceDataStores>
    <targetTypes>
        <FeatureType>
            <schemaUri>http://icsm.govspace.gov.au/files/2015/05/GeodesyML.xsd</schemaUri>
        </FeatureType>
        <FeatureType>
            <schemaUri>http://schemas.opengis.net/gml/3.2.1/gml.xsd</schemaUri>
        </FeatureType>
    </targetTypes>
    <typeMappings>
        <FeatureTypeMapping>
            <sourceDataStore>surveymarks</sourceDataStore>
            <sourceType>adjustments_simple</sourceType>
            <targetElement>geo:Adjustment</targetElement>
            <attributeMappings>
                <AttributeMapping>
                    <targetAttribute>
                        geo:Adjustment
                    </targetAttribute>
                    <idExpression>
                        <OCQL>gid</OCQL>
                    </idExpression>
                    <ClientProperty>
                        <name>srsName</name>
                        <value>strConCat('urn:ogc:def:crs:EPSG:6.9:',d_epsg_code)</value>
                    </ClientProperty>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        gml:identifier
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>adj_id</OCQL>
                    </sourceExpression>
                    <ClientProperty>
                        <name>codeSpace</name>
                        <value>'urn:vic-gov-au:icsm:egeodesy:geodesyml_adjustment-id'</value>
                    </ClientProperty>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        gml:validTime/gml:TimeInstant/gml:timePosition
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>adj_epoch</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        geo:runDate
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>adj_date</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        geo:runBy/gmd:CI_ResponsibleParty/gmd:role/gmd:CI_RoleCode
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>adj_runby</OCQL>
                    </sourceExpression>
                    <ClientProperty>
                        <name>codeList</name>
                        <value>'urn:vic-gov-au:icsm:egeodesy:geodesyml_adjustment-runby'</value>
                    </ClientProperty>
                    <ClientProperty>
                        <name>codeListValue</name>
                        <value>'urn:vic-gov-au:icsm:egeodesy:geodesyml_adjustment-runby'</value>
                    </ClientProperty>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        geo:results/geo:adjustmentValue
                      </targetAttribute>
                    <sourceExpression>
                        <OCQL>dof</OCQL>
                    </sourceExpression>
                    <isMultiple>true</isMultiple>
                    <ClientProperty>
                        <name>codeSpace</name>
                        <value>'urn:vic-gov-au:icsm:egeodesy:geodesyml_adjustment-dof'</value>
                    </ClientProperty>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        geo:results/geo:adjustmentValue[2]
                      </targetAttribute>
                    <sourceExpression>
                        <OCQL>chi_square</OCQL>
                    </sourceExpression>
                    <isMultiple>true</isMultiple>
                    <ClientProperty>
                        <name>codeSpace</name>
                        <value>'urn:vic-gov-au:icsm:egeodesy:geodesyml_adjustment-chi-square'</value>
                    </ClientProperty>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        geo:results/geo:adjustmentValue[3]
                      </targetAttribute>
                    <sourceExpression>
                        <OCQL>sigma_zero</OCQL>
                    </sourceExpression>
                    <isMultiple>true</isMultiple>
                    <ClientProperty>
                        <name>codeSpace</name>
                        <value>'urn:vic-gov-au:icsm:egeodesy:geodesyml_adjustment-sigma-zero'</value>
                    </ClientProperty>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        geo:results/geo:adjustmentValue[4]
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>lower_limit</OCQL>
                    </sourceExpression>
                    <isMultiple>true</isMultiple>
                    <ClientProperty>
                        <name>codeSpace</name>
                        <value>'urn:vic-gov-au:icsm:egeodesy:geodesyml_adjustment-lower-limit'</value>
                    </ClientProperty>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        geo:results/geo:adjustmentValue[5]
                      </targetAttribute>
                    <sourceExpression>
                        <OCQL>upper_limit</OCQL>
                    </sourceExpression>
                    <isMultiple>true</isMultiple>
                    <ClientProperty>
                        <name>codeSpace</name>
                        <value>'urn:vic-gov-au:icsm:egeodesy:geodesyml_adjustment-upper-limit'</value>
                    </ClientProperty>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        geo:adjustmentProcess/gml:scope
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>adj_type_txt</OCQL>
                    </sourceExpression>
                    <ClientProperty>
                        <name>codeSpace</name>
                        <value>'urn:vic-gov-au:icsm:egeodesy:geodesyml_adjustment-type-description'</value>
                    </ClientProperty>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        geo:adjustmentProcess/geo:process/geo:description
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>'Dynanet'</OCQL>
                    </sourceExpression>
                    <ClientProperty>
                        <name>codeSpace</name>
                        <value>'urn:vic-gov-au:icsm:egeodesy:geodesyml_adjustment-process-description'</value>
                    </ClientProperty>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        geo:configureValue
                      </targetAttribute>
                    <sourceExpression>
                        <OCQL>confidence_interval</OCQL>
                    </sourceExpression>
                    <ClientProperty>
                        <name>codeSpace</name>
                        <value>'urn:vic-gov-au:icsm:egeodesy:geodesyml_adjustment-confidence-interval'</value>
                    </ClientProperty>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        geo:epoch
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>adj_epoch</OCQL>
                    </sourceExpression>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        geo:Status/geo:currentStatus/geo:statusCode
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>'Authoritative'</OCQL>
                    </sourceExpression>
                    <ClientProperty>
                        <name>codeSpace</name>
                        <value>'urn:vic-gov-au:icsm:egeodesy:geodesyml_adjustment-status'</value>
                    </ClientProperty>
                </AttributeMapping>
                <AttributeMapping>
                    <targetAttribute>
                        geo:Status/geo:currentStatus/gml:validTime/gml:TimeInstant/gml:timePosition
                    </targetAttribute>
                    <sourceExpression>
                        <OCQL>adj_date</OCQL>
                    </sourceExpression>
                </AttributeMapping>
            </attributeMappings>
        </FeatureTypeMapping>
    </typeMappings>
</as:AppSchemaDataAccess>
